{
  "name": "nest-typescript-starter",
  "private": true,
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "NODE_ENV=development nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "NODE_ENV=production node dist/src/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "db:sync:dev": "npx prisma db push",
    "db:seed": "npx prisma db seed --preview-feature",
    "db:migrate:dev:createOnly": "prisma migrate dev --create-only",
    "db:migrate:dev": "npx prisma migrate dev",
    "db:migrate:prod": "npx prisma migrate deploy"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^1.6.0",
    "@nestjs/bull": "^0.3.1",
    "@nestjs/common": "^7.6.15",
    "@nestjs/core": "^7.6.15",
    "@nestjs/platform-express": "^7.6.15",
    "@nestjs/swagger": "^4.4.0",
    "@prisma/client": "^2.23.0",
    "@sendgrid/mail": "^7.2.5",
    "@types/bcryptjs": "^2.4.2",
    "app-root-path": "^3.0.0",
    "axios": "^0.20.0",
    "bcryptjs": "^2.4.3",
    "bull": "^3.22.7",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "date-fns": "^2.22.1",
    "dotenv": "^8.2.0",
    "googleapis": "^59.0.0",
    "handlebars": "^4.7.7",
    "http-status-codes": "^2.1.3",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "morgan": "^1.9.1",
    "nodemailer": "^6.6.1",
    "onesignal-node": "^3.1.5",
    "passport": "^0.4.1",
    "passport-google-oauth20": "^2.0.0",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
    "pg": "^8.3.3",
    "pg-hstore": "^2.3.3",
    "prisma": "^2.23.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.6",
    "sequelize": "^6.3.5",
    "sequelize-cli": "^6.2.0",
    "sequelize-typescript": "^1.1.0",
    "swagger-ui-express": "^4.1.4"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.11",
    "@nestjs/cli": "^7.6.0",
    "@nestjs/schematics": "^7.3.1",
    "@nestjs/testing": "^7.6.15",
    "@types/app-root-path": "^1.2.4",
    "@types/bcrypt": "^5.0.0",
    "@types/bull": "^3.15.1",
    "@types/connect-pg-simple": "^4.2.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.11",
    "@types/express-session": "^1.17.0",
    "@types/jest": "^26.0.22",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.14.36",
    "@types/nodemailer": "^6.4.2",
    "@types/passport": "^1.0.4",
    "@types/passport-local": "^1.0.33",
    "@types/request-promise": "^4.1.46",
    "@types/supertest": "^2.0.10",
    "@types/umzug": "^2.2.3",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.4",
    "ts-loader": "^8.0.18",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.3",
    "umzug": "^2.3.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
